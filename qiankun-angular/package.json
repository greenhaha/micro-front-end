{
  "name": "qiankun-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check --port 10300 --base-href /angular",
    "build": "ng build --prod --deploy-url http://localhost:10300",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:single-spa:micro-app-angular": "ng build micro-app-angular --prod --deploy-url http://localhost:10300/",
    "serve:single-spa:qiankun-angular": "ng s --project qiankun-angular --disable-host-check --port 10300 --live-reload false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "ng-zorro-antd": "^12.0.1",
    "rxjs": "~6.6.0",
    "single-spa": "^5.9.3",
    "single-spa-angular": "^5.0.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.1",
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5",
    "zone.js": "^0.11.4"
